<% include partials/header%>
    <nav class="navbar fixed-top navbar-expand-lg navbar-dark black">
        <a class="navbar-brand" href="http://www.startereum.com/">
            <img src="" width="50" height="50" alt="">

            <span style="vertical-align: middle;color: #B49353">Starterium</span>
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup"
            aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">
                <a class="nav-item nav-link" href="/">Home</a>
                <a class="nav-item nav-link" href="/game">Game</a>
                <a class="nav-item nav-link active" href="/register">Sign Up</a>
            </div>
        </div>
    </nav>
    <div class="container justify-content-center" style="position: relative; top: 70px; color: white">
        <h1>Sign Up Form</h1>
        <form>
            <div class="form-group">
                <label for="publicKey">Public Key</label>
                <input name="username" class="form-control" id="publicKey" readonly>
            </div>
            <div class="form-group">
                <input name="password" class="form-control d-none" id="publicKey2" placeholder="Public Key" readonly>
            </div>
            <div class="form-group">
                <label for="email">Email address</label>
                <input name="email" type="email" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Enter email">
                <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
            </div>
            <button type="submit" id="signup_btn" class="btn btn-warning">Signup with Metamask</button>
        </form>
    </div>

    <% include partials/footer%>
    <script src="js/web3.min.js "></script>
    <script src="js/register.js "></script>

    </body>
</html>