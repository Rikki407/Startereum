                                <a class="btn-reply">
                                    <i class="fa fa-reply"></i>
                                </a>
                            </div>
                        </div>
                        <div class="comment-footer">
                            <div class="comment-form">
                                <textarea class="form-control" name="" id=""></textarea>
                                <div class="pull-right send-button">
                                    <a class="btn-send">send</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <ul class="comments-list reply-list">

                </ul>
            </li>

